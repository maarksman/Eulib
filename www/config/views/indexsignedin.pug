    title Eulib
    // Bootstrap core CSS
    link(href='vendor/bootstrap/css/bootstrap.css', rel='stylesheet')
    // <link href="vendor/bootstrap/css/bootstrap.css" rel="stylesheet">
    // Custom styles for this template
    link(href='css/small-business.css', rel='stylesheet')
    link(href='css/searchbar.css', rel='stylesheet')
    link(href='//fonts.googleapis.com/css?family=Open+Sans:400italic,700italic,400,700|Merriweather:400,700,900', rel='stylesheet')
    // article search
    script(src='http://code.jquery.com/jquery-1.11.3.min.js')
    script(type='text/javascript', src='js/searcharticle.js')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/mathjax/2.7.2/MathJax.js?config=TeX-MML-AM_CHTML')

    script(type='text/javascript', src='js/articlecreate.js')
    script(src='js/signedin.js')

    link(href='http://aimath.org/knowlstyle.css', type="text/css", rel='stylesheet')
    script(type='text/javascript', src='http://aimath.org/knowl.js')
    script(src='https://code.jquery.com/jquery-2.1.3.min.js')
    script(src='http://maxcdn.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js')
    link(href='css/leftright.css', rel='stylesheet')
  body
    // Navigation
    nav.navbar.navbar-expand-lg.navbar-dark.bg-gray.fixed-top
      .container
        a.navbar-brand(href='/indexsignedin') Eulib
        
        button.navbar-toggler(type='button', data-toggle='collapse', data-target='.navbar-collapse', aria-controls='navbarResponsive', aria-expanded='false', aria-label='Toggle navigation')
          span.navbar-toggler-icon
            |
            span.icon-bar
            |
            span.icon-bar
            |
            span.icon-bar
              
        #navbarResponsive.collapse.navbar-collapse
          ul.navbar-nav.ml-auto
            form.searchbox.sbx-twitterr(novalidate='novalidate', onsubmit='return false;')
              .sbx-twitter__wrapper(role='search')
                
                datalist#articles
                input#search.sbx-twitter__input(type='search', name='search', placeholder='Search your website', autocomplete='off', required='required', list='articles')
                button#search_button.sbx-twitter__submit(type='submit', title='Submit your search query.')
                  svg(role='img', aria-label='Search')
                    use(xmlns:xlink='http://www.w3.org/1999/xlink', xlink:href='#sbx-icon-search-8')
                button.sbx-twitter__reset(type='reset', title='Clear the search query.')
                  svg(role='img', aria-label='Reset')
                    use(xmlns:xlink='http://www.w3.org/1999/xlink', xlink:href='#sbx-icon-clear-3')
                button#search_all(type='submit', title='Search all suggestions.') Submit All
                button#clearAllButton(onClick='clearAllKnowls()') Clear All
            li.nav-item
              a.nav-link(href='#' id = "addknowl") Add knowl
            //- li.nav-item
            //-   a.nav-link(href='new_sign_in_page.html') Login
            //- li.nav-item
            //-   a.nav-link(href='html/new_sign_up_page.html') Sign up
            li.nav-item
              a.nav-link(href='http://localhost:3000/signedin') #{name}'s Profile
            li.nav-item
              button#signout(type='button') Sign-out
    .container
      #dialogbox
      #articles-searched
        #entry-content.boxed(,='', align='center')
          | What would you like to know?
          p
      .newarticle
        .minimizebar 
        .closebar X
        lebel(for='name')
          span.required Title: *
          input#title(type='text', name='title', class='#')
        label(for='type')
          span.required Type: *
          select#type(name='type', tabindex='4')
            option(value='Article') Article
            option(value='Paper') Paper
            option(value='Poster') Poster
        div
          label(for='field')
            span.required Field: *
            input#field(name='field', list="fieldlist")
            datalist#fieldlist
        label(for='level')
          span.required Level: *
          select#level(name="level")
            option(value='1') 1
            option(value='2') 2
            option(value='3') 3
            option(value='4') 4
            option(value='5') 5
        br
        input#file-select(type="file", name="image")
        div
          span Add knowl link: 
          input#addlink(list='linkstoadd', name='titlelist')
          br
          datalist#linkstoadd
          button#addknowlbutton Generate link  
          br
          br
          span Highlight the text you want to link: 
          br
          span#linkmessage(style="padding-left:15px;color:red;")
          br
          br
        label(for='content')
          span.required Content: *
          br
          textarea#content(cols="60", rows="4", name='content', class='#', placeholder='Please write your article here.', tabindex='5', required='required')
        button#submit(name='submit', value="Submit") Submit
        span#returnmessage(style="padding-left:15px;color:red;") 
      .maximizebar
        span.maxicon
          | 
          span
